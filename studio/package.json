  "dependencies": {
    "@sanity/base": "^2.7.4",
    "@sanity/cli": "^2.7.4",
    "@sanity/components": "^2.2.6",
    "@sanity/core": "^2.7.4",
    "@sanity/dashboard": "^2.7.0",
    "@sanity/default-layout": "^2.7.4",
    "@sanity/default-login": "^2.7.0",
