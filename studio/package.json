    "date-fns": "^2.19.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^3.11.0",
    "sanity-plugin-dashboard-widget-document-list": "^0.0.13",
    "sanity-plugin-dashboard-widget-netlify": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "@webstackbuilders/eslint-config-company-website",
