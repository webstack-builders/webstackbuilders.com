{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "cSpell.ignorePaths": [
    "**/__generated__/**",
    "**/.git/**",
    "**/.vscode",
    "**/node_modules/**",
    "**/package-lock.json"
  ],
  "cSpell.words": [
    "avif",
    "blog",
    "browserslistrc",
    "camelcase",
    "contentful",
    "corejs",
    "es",
    "gtag",
    "myorg",
    "navigations",
    "post",
    "react's",
    "repos",
    "symlink",
    "symlinking",
    "toolchain",
    "true",
    "tsbuildinfo",
    "typegen",
    "unmount",
    "unmounting"
  ],
  "cSpell.ignoreWords": [
    "avi",
    "dont",
    "nlink"
  ],
  "git.ignoreLimitWarning": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/__fixtures__": true,
    "/web/public": true,
    "/studio/build": true
  }
}
