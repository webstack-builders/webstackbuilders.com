{
  "private": true,
  "name": "webstack-builders-company-website",
  "description": "Gatsby frontend for the Webstack Builders company website",
  "author": "Webstack Builders <info@webstackbuilders.com>",
  "homepage": "https://webstackbuilders.com",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webstackdev/webstackbuilders.com.git"
  },
  "bugs": "https://github.com/webstackdev/webstackbuilders.com/issues",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0",
    "yarn": "please-use-npm"
  },
  "packageManager": "npm",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths",
    "dev": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop -p 8002",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "lint": "eslint --format=autolinkable-stylish .",
    "test": "npm run lint && jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/skip-nav": "^0.16.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^1.0.1",
    "@theme-ui/prism": "^0.11.2",
    "@webstackbuilders/theme-preset-website": "^0.4.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.19.0",
    "gatsby": "^3.13.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-canonical-urls": "^3.13.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-layout": "^2.10.0",
    "gatsby-plugin-manifest": "^3.12.0",
    "gatsby-plugin-netlify": "^3.10.0",
    "gatsby-plugin-postcss": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-theme-ui": "^0.11.2",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.13.0",
    "gatsby-source-sanity": "^7.0.7",
    "gatsby-transformer-sharp": "^3.13.0",
    "modern-normalize": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "theme-ui": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/classnames": "^2.3.1",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.2",
    "@types/react-test-renderer": "^17.0.1",
    "@webstackbuilders/eslint-config-company-website": "^0.4.0",
    "@webstackbuilders/prettier-config-company-website": "^0.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^1.13.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gatsby-plugin-eslint": "^4.0.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "postcss": "^8.2.7",
    "postcss-preset-env": "^6.6.0",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "prettier": "@webstackbuilders/prettier-config-company-website"
}
