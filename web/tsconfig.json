{
  "extends": "../tsconfig.json",
  "include": [
    "./gatsby-node.ts",
    "./gatsby-browser.tsx",
    "./gatsby-ssr.tsx",
    "src/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "emitDeclarationOnly": true,
    "module": "esnext",
    "target": "esnext",
    "paths": {
      "api/*": ["src/api/*"],
      "components/*": ["src/components/*"],
      "containers/*": ["src/containers/*"],
      "gatsby/*": ["src/gatsby/*"],
      "layouts/*": ["src/layouts/*"],
      "lib/*": ["src/lib/*"],
      "pages/*": ["src/pages/*"],
      "styles/*": ["src/styles/*"],
      "templates/*": ["src/templates/*"]
    }
  }
}
